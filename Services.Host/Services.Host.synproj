    <OutputPath>bin\$(Platform)\$(Configuration)</OutputPath>
    <DocumentationFile>bin\$(Platform)\$(Configuration)\Services.Host.xml</DocumentationFile>
  </PropertyGroup>
  <ItemGroup>
